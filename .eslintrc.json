{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "react/no-array-index-key": "off",
    "linebreak-style": "off",
    "no-useless-escape": "off",
    "react/forbid-prop-types": "off",
    "no-console": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/jsx-props-no-spreading": "off"
  },
  "globals": {
    "eternals": true,
    "_": true
  }
}